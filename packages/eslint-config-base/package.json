{
  "name": "@seanblonien/eslint-config-base",
  "version": "1.0.1",
  "description": "Sean Blonien's opinionated ESLint config for TypeScript projects",
  "keywords": [
    "eslint",
    "eslint-config",
    "flat-config",
    "javascript",
    "typescript"
  ],
  "homepage": "https://github.com/seanblonien/seanblonien-eslint-config/tree/main/packages/eslint-config-base#readme",
  "bugs": {
    "url": "https://github.com/seanblonien/seanblonien-eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seanblonien/seanblonien-eslint-config"
  },
  "license": "MIT",
  "author": "Sean Blonien",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
