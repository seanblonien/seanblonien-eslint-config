{
  "name": "seanblonien-eslint-config",
  "version": "0.0.0",
  "private": true,
  "description": "Monorepo for Sean Blonien's opinionated ESLint config for TypeScript & React projects",
  "homepage": "https://github.com/seanblonien/seanblonien-eslint-config#readme",
  "bugs": {
    "url": "https://github.com/seanblonien/seanblonien-eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seanblonien/seanblonien-eslint-config"
  },
  "type": "module",
  "scripts": {
    "build": "turbo build",
    "changeset": "changeset",
    "clean": "turbo clean && rm -rf node_modules",
    "clean:all": "find . -type d \\( -name dist -o -name .turbo \\) -prune -exec rm -rf {} +",
    "lint": "turbo lint",
    "release": "pnpm build && changeset publish",
    "sort": "npx sort-package-json \"package.json\" \"packages/*/package.json\"",
    "test": "turbo test",
    "validate": "turbo lint && turbo build && turbo test",
    "version-packages": "changeset version"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "eslint": "^9.39.1",
    "jiti": "^2.6.1",
    "turbo": "^2.6.1",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.22.0+sha512.bf049efe995b28f527fd2b41ae0474ce29186f7edcb3bf545087bd61fbbebb2bf75362d1307fda09c2d288e1e499787ac12d4fcb617a974718a6051f2eee741c",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  },
  "pnpm": {
    "overrides": {
      "read-yaml-file": "^2.1.0"
    }
  }
}
