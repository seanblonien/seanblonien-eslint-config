{
  "name": "seanblonien-eslint-config",
  "version": "0.0.0",
  "private": true,
  "description": "Monorepo for Sean Blonien's opinionated ESLint config for TypeScript & React projects",
  "homepage": "https://github.com/seanblonien/seanblonien-eslint-config#readme",
  "bugs": {
    "url": "https://github.com/seanblonien/seanblonien-eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seanblonien/seanblonien-eslint-config"
  },
  "type": "module",
  "scripts": {
    "build": "turbo build",
    "clean": "turbo clean && rm -rf node_modules",
    "clean:all": "find . -type d \\( -name dist -o -name .turbo \\) -prune -exec rm -rf {} +",
    "commitlint": "commitlint --edit",
    "lint": "eslint . --fix",
    "prepare": "husky",
    "release": "pnpm build && multi-semantic-release",
    "sort": "npx sort-package-json \"package.json\" \"packages/*/package.json\"",
    "test": "turbo test",
    "validate": "pnpm lint && turbo build && turbo test"
  },
  "devDependencies": {
    "@anolilab/semantic-release-pnpm": "^3.2.2",
    "@commitlint/cli": "^20.3.0",
    "@commitlint/config-conventional": "^20.3.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.2",
    "@semantic-release/npm": "^13.1.3",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "multi-semantic-release": "^3.1.0",
    "semantic-release": "^25.0.2",
    "turbo": "^2.7.3",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.27.0+sha512.72d699da16b1179c14ba9e64dc71c9a40988cbdc65c264cb0e489db7de917f20dcf4d64d8723625f2969ba52d4b7e2a1170682d9ac2a5dcaeaab732b7e16f04a",
  "engines": {
    "node": ">=24.0.0",
    "pnpm": ">=10.0.0"
  },
  "pnpm": {
    "overrides": {
      "read-yaml-file": "^2.1.0"
    },
    "allowedDeprecatedVersions": {
      "@babel/plugin-proposal-private-methods": "7.18.6",
      "semver-diff": "5.0.0"
    }
  }
}
